{
  "$schema": "https://json.schemastore.org/package",
  "name": "@xeho91/assets",
  "version": "1.1.1",
  "description": "xeho91's brand assets with custom CLI for automated tasks.",
  "keywords": [
    "brand assets",
    "avatar",
    "logo",
    "automation",
    "SVG",
    "SMIL",
    "animated",
    "PNG",
    "Svelte",
    "CLI"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Mateusz Kadlubowski",
    "email": "xeho91@pm.me",
    "url": "https://xeho91.com/"
  },
  "homepage": "https://github.com/xeho91/assets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/assets.git"
  },
  "bugs": {
    "url": "https://github.com/xeho91/assets/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "xeho91": "./bin/cli.js"
  },
  "type": "module",
  "svelte": "./dist/components.js",
  "exports": "./dist/components.js",
  "types": "./dist/components.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/examples"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "check": "run-p check:*",
    "check:ts": "tsc",
    "check:svelte": "svelte-check",
    "dev": "pnpm build -- --watch",
    "generate": "run-p generate:*",
    "generate:avatar": "./bin/cli.js generate avatar --all --output ./dist/avatar/svg",
    "generate:logo": "./bin/cli.js generate logo --all --output ./dist/logo/svg",
    "generate:types": "svelte-type-generator && dts-bundle --name components --main ./types/components/index.d.ts && cpy ./types/components/components.d.ts ./dist/",
    "release": "standard-version"
  },
  "dependencies": {
    "@xeho91/colors": "^1.2.0",
    "chalk": "^4.1.1",
    "enquirer": "^2.3.6",
    "listr2": "^3.10.0",
    "standard-version": "^9.3.0",
    "svgo": "2.3.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.12.2",
    "@types/svgo": "^2.3.0",
    "@types/yargs": "^17.0.0",
    "cpy-cli": "^3.1.1",
    "dts-bundle": "^0.7.3",
    "esbuild": "^0.12.9",
    "fs-extra": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.52.1",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-svelte": "^7.1.0",
    "standard-changelog": "^2.0.27",
    "svelte": "^3.38.2",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "svelte-type-generator": "^1.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.3"
  }
}
